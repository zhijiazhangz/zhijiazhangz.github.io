function findgenmat(L,i,f,mm,K)
    actG:=L[i]@@f;ngeg:=[];
    for x in Generators(actG) do 
                ssma:=mm(x);
                ssma:=DiagonalJoin(ScalarMatrix(K,1,x[1,1]),ssma);
                Append(~ngeg,ssma);
    end for;
    return ngeg;
end function;

